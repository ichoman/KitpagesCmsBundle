<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="ano_user.repository.user.class">Ano\Bundle\UserBundle\Repository\DoctrineUserRepository</parameter>
    </parameters>

    <services>
        <service id="ano_user.repository.user" class="%ano_user.repository.user.class%">
            <argument type="service" id="doctrine.orm.entity_manager" />
            <argument>%ano_user.model.user.class%</argument>
        </service>
    </services>
    
</container>

    <h1>choose the type of page to create</h1>
    <ul>
        <li><a href="{{ path('kitpages_cms_page_create', { 'parent_id': parent_id, 'next_sibling_slug' : next_sibling_slug, 'prev_sibling_slug' : prev_sibling_slug, 'kitpages_target': kitpages_target }) }}">create page</a></li>
